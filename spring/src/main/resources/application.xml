<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">



    <bean id="myBeanPostProcessor" class="com.walter.service.MyBeanPostProcessor"/>
    <bean id="userDaoImpl" class="com.walter.dao.StudentDaoImpl"/>
    <bean id="studentServiceImpl" class="com.walter.service.StudentServiceImpl">
        <property name="studentDao" ref="userDaoImpl"/>
    </bean>
</beans>